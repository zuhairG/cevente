<p id="notice"><%= notice %></p>
<div class="container">
    <div class="row">
<h1>Listing User Orgs</h1>
</div>
</div>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_orgs.each do |user_org| %>
      <tr>
        <td><%= user_org.user_id %></td>
        <td><%= user_org.organization_id %></td>
        <td><%= link_to 'Show', user_org %></td>
        <td><%= link_to 'Edit', edit_user_org_path(user_org) %></td>
        <td><%= link_to 'Destroy', user_org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User org', new_user_org_path %>
